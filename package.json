  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:hosted": "prisma generate --schema=prisma/schema.hosted.prisma && next build",
    "db:deploy:hosted": "prisma migrate deploy --schema=prisma/schema.hosted.prisma",
    "db:push:hosted": "prisma db push --schema=prisma/schema.hosted.prisma",
    "db:diff:hosted": "prisma migrate diff --from-url \"$DATABASE_URL\" --to-schema-datamodel prisma/schema.hosted.prisma --script",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@types/uuid": "^10.0.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.556.0",
    "next": "14.2.3",
    "react": "^18",
    "react-dom": "^18",
    "react-markdown": "^10.1.0",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
